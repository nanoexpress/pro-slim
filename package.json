    "baretest": "^2.x",
    "commitlint-config-airlight": "^1.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.17.0",
    "eslint-config-airlight-node": "^1.1.7",
    "husky": "^4.3.7",
    "husky-config-airlight": "^1.0.2",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
