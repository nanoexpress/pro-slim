{
  "name": "@nanoexpress/pro-slim",
  "version": "3.0.0",
  "description": "nanoexpress for enterprise users & teams",
  "type": "module",
  "main": "src/nanoexpress.ts",
  "module": "esm/nanoexpress.js",
  "typings": "typings/nanoexpress.d.ts",
  "scripts": {
    "lint": "smartlint",
    "test": "node --experimental-modules tests/index.js",
    "cleanup": "rm -rf cjs/*.js cjs/*.js.map esm/*.js esm/*.js.map typings/",
    "build": "npm run cleanup && rollup -c",
    "prepublishOnly": "yarn lint && yarn build"
  },
  "engines": {
    "node": ">=12.16"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanoexpress/pro-slim.git"
  },
  "keywords": [
    "nano",
    "node",
    "fast",
    "uws",
    "websocket",
    "http",
    "c++"
  ],
  "author": "dalisoft",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/nanoexpress/pro-slim/issues"
  },
  "homepage": "https://github.com/nanoexpress/pro-slim#readme",
  "dependencies": {
    "fast-decode-uri-component": "^1.0.1",
    "path-to-regexp": "^6.2.0",
    "uWebSockets.js": "https://github.com/uNetworking/uWebSockets.js/archive/v19.5.0.tar.gz"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.0",
    "@rollup/plugin-commonjs": "^21.0.0",
    "@rollup/plugin-node-resolve": "^13.0.5",
    "@types/node": "^16.10.2",
    "baretest": "^2.x",
    "commitlint-config-airlight": "^1.0.4",
    "coveralls": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-airlight-node": "^1.1.9",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "lint-staged-config-airlight": "^2.0.1",
    "prettier": "^2.4.1",
    "prettier-config-airlight": "^1.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-typescript2": "^0.30.0",
    "smartlint": "^2.1.3",
    "typedoc": "^0.22.5",
    "typescript": "^4.2.x"
  }
}
